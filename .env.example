# Agent Diplomacy Environment Configuration

# Server
PORT=3000
NODE_ENV=development

# Database
DB_PATH=./db/games.db

# Security
JWT_SECRET=your-secret-key-change-in-production
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Game Settings
DEFAULT_MAX_TURNS=50
DEFAULT_PHASE_DURATION_NEGOTIATION=60000
DEFAULT_PHASE_DURATION_COMMIT=30000
DEFAULT_PHASE_DURATION_RESOLVE=10000

# Logging
LOG_LEVEL=info
ENABLE_DEBUG_LOGS=false